   "source": [
    "import numpy as np\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "import math"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "id": "1981a65f",
   "metadata": {},
   "outputs": [],
   "source": [
    "# test set. Error is distributed normally with mean 0, sd sqrt(0.5).\n",
    "D_test = np.random.uniform(-1,1,size=10000)\n",
    "y_test = np.sin(math.pi*D_test) +  np.random.normal(0,np.sqrt(0.5),size=10000)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "id": "bb1b0a7c",
   "metadata": {},
   "outputs": [],
   "source": [
    "# 500 training sets\n",
    "S = np.random.uniform(-1,1,(500,50))\n",
    "y_train = np.sin(math.pi*S) +  np.random.normal(0,np.sqrt(0.5),(500,50))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 45,
   "id": "2200e821",
   "metadata": {},
   "outputs": [],
   "source": [
    "#array to store the bias and variance for various p values.\n",
    "bias,variance = [],[]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 46,
   "id": "eb861088",
   "metadata": {},
   "outputs": [],
   "source": [
    "for p in range(1,11):\n",
    "    beta_hats = []\n",
    "    for i in range(len(S)):\n",
    "        X = np.ones(50).reshape(50,1)\n",
    "        for j in range(1,p+1):\n",
    "            X = np.append(X,S[i].reshape(50,1)**j,axis=1)\n",
    "        XTX = X.T@X\n",
    "        beta_hat = np.dot(np.linalg.inv(XTX)@X.T,y_train[i])\n",
    "        beta_hats.append(beta_hat)\n",
    "    beta_hats = np.array(beta_hats)\n",
    "    \n",
    "    bias_p,variance_p,f_hat_p = 0,0,beta_hats.mean(0)\n",
    "    for k in range(len(D_test)):\n",
    "        x = np.array([D_test[k]**i for i in range(p+1)])\n",
    "        bias_p += (np.sin(math.pi*D_test[k])-np.dot(f_hat_p,x))**2\n",
    "    \n",
    "        for i in beta_hats:\n",
    "            variance_p += (np.dot(i,x)-np.dot(f_hat_p,x))**2\n",
    "    bias.append(bias_p/10000)\n",
    "    variance.append(variance_p/(10000*500))\n",
    "        "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 48,
   "id": "00f2acf5",
   "metadata": {},
   "outputs": [
