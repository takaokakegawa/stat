    "plt.plot(range(1,11),bias,label=\"bias\")\n",
    "plt.plot(range(1,11),variance,label=\"variance\")\n",
    "plt.xlabel(\"p\")\n",
    "plt.title(\"Bias-Variance tradeoff\")\n",
    "plt.legend(loc=\"upper left\")"
   ]
